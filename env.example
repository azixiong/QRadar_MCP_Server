# QRadar MCP Server Configuration
# Copy this file to .env and fill in your actual values

# QRadar Host Address (required)
# Example: https://qradar.example.com
QRADAR_HOST=https://your-qradar-instance.com

# QRadar Username (required)
QRADAR_USERNAME=your-username

# QRadar Password (required)
QRADAR_PASSWORD=your-password

# Request Timeout in seconds (optional, default: 30)
QRADAR_TIMEOUT=30

# SSL Certificate Verification (optional, default: true)
# Set to "false" only for development/testing with self-signed certificates
QRADAR_VERIFY_SSL=true

